{
  "allowCompoundWords": true,
  "dictionaries": [
    "user",
    "go",
    "rust",
    "bash",
    "typescript",
  ],
  "dictionaryDefinitions": [
    {
      "name": "user",
      "path": "~/.config/cspell/user.txt",
      "description": "User local dictionary"
    }
  ],
  "languageSettings": [
    {
      "languageId": "lua",
      "locale": "*",
      "ignoreRegExpList": [
        "/require.*/"
      ],
      "dictionaries": [
        "lua"
      ]
    },
    {
      "languageId": "vim",
      "locale": "*",
      "ignoreRegExpList": [
        "/Plug .*/"
      ],
      "dictionaries": [
        "vim"
      ]
    }
  ]
}
